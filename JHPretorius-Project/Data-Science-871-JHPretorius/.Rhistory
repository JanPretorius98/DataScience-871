ggplot(aes(x = counts_profileVisits, y = counts_kisses)) +
geom_point(aes(fill = factor(has_emoji), size = counts_details), pch = 21, alpha = 0.5) +
scale_fill_manual(values = c("#29066b", "#ea7369"), name = "Has Emoji", labels = c("No Emoji", "Contains Emoji")) +
