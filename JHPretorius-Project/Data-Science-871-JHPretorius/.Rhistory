library(vars)
library(tidyverse)
library(ggplot2)
# Load the data
data(Canada)
# Estimate a VAR
var_model <- VAR(Canada, p = 2)
# Calculate the impulse responses
irf_results <- irf(var_model, impulse = "rw", response = "U", n.ahead = 20, boot = TRUE, ci = 0.95)
# Reshape the data for ggplot
irf_df <- data.frame(Time = 1:21,
Response = irf_results$irf$U,
Lower = irf_results$Lower$U,
Upper = irf_results$Upper$U)
